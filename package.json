{
  "name": "grafana-mqtt-datasource",
  "version": "1.1.0-beta.1",
  "description": "MQTT Datasource Plugin",
  "scripts": {
    "build": "rm -rf dist && grafana-toolkit plugin:build && mage build:backend",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "grafana-toolkit plugin:test && mage test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "9.1.0",
    "@grafana/e2e-selectors": "9.1.0",
    "@grafana/experimental": "1.7.11",
    "@grafana/runtime": "9.1.0",
    "@grafana/toolkit": "9.1.0",
    "@grafana/ui": "9.1.0",
    "@types/lodash": "latest",
    "aedes": "^0.45.0",
    "cspell": "6.13.3",
    "rxjs": "7.8.0"
  },
  "engines": {
    "node": ">=16"
  }
}
