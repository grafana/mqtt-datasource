{
  "name": "grafana-mqtt-datasource",
  "version": "1.2.0",
  "description": "MQTT Datasource Plugin",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "start": "yarn dev",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "broker:pki": "./scripts/gen_pki.sh",
    "broker:tls": "node scripts/test_broker.js --tls",
    "broker": "node scripts/test_broker.js",
    "server": "docker compose up --build"
  },
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@changesets/cli": "2.29.5",
    "@grafana/eslint-config": "8.0.0",
    "@grafana/plugin-e2e": "2.0.5",
    "@grafana/tsconfig": "2.0.0",
    "@playwright/test": "1.53.0",
    "@stylistic/eslint-plugin-ts": "2.13.0",
    "@swc/core": "1.7.14",
    "@swc/helpers": "0.5.12",
    "@swc/jest": "0.2.36",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.14",
    "@types/node": "20.16.1",
    "@types/react-router-dom": "5.3.3",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "aedes": "0.51.3",
    "copy-webpack-plugin": "11.0.0",
    "cspell": "6.13.3",
    "css-loader": "6.11.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-jsdoc": "46.10.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-webpack-plugin": "4.2.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "glob": "10.4.5",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "5.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.5.3",
    "replace-in-file-webpack-plugin": "1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "semver": "7.7.2",
    "style-loader": "3.3.3",
    "swc-loader": "0.2.6",
    "terser-webpack-plugin": "5.3.14",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.4",
    "webpack": "5.99.9",
    "webpack-cli": "5.1.4",
