    "@grafana/runtime": "^12.0.1",
    "@grafana/schema": "^12.0.1",
    "@grafana/ui": "^12.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.5.3"
  },
  "packageManager": "yarn@4.9.1"
